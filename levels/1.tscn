resource_name = "blocks"
script/source = "@tool
extends StaticBody2D

# Called every frame. 'delta' is the elapsed time since the previous frame.
func add_sprites():
		for child in get_children():
			if child.get_child_count() == 0:
				print('children dont exist, making new one')
				child.add_child(Sprite2D.new())
				child.add_child(LightOccluder2D.new())
				print(child.get_children())
				if child.get_child_count() > 2:
					print(child.name, ' HAS TOO MANY CHILDREN!!!! REMOVING CHILDREN!!!!', child.get_children())
					for child2 in child.get_children():
						child.remove_child(child2)
			else:
				print('child exists')
''
func update_sprites():
	for child in get_children():
		var shape = child.shape.get_rect()
		var sprite = child.get_child(0)
		var occuluder = child.get_child(1)
		
		if not child.disabled:
			var material = CanvasItemMaterial.new()
			material.light_mode = CanvasItemMaterial.LIGHT_MODE_UNSHADED
			sprite.material = material
		
		sprite.texture = load(\"res://assets/cube.png\")
		sprite.texture_repeat = CanvasItem.TEXTURE_REPEAT_ENABLED
		if child.disabled:
			sprite.modulate = Color(.5, .5, .5)
		elif child.one_way_collision:
			sprite.modulate = Color(.8, .8, .8)
		sprite.scale = Vector2(.25, .25)
		sprite.region_enabled = true
		sprite.region_rect = Rect2(
			Vector2(0,0),
			shape.size*4
		)

		if not child.disabled:
			var polygon = OccluderPolygon2D.new()
			polygon.polygon = PackedVector2Array([
				shape.position,
				Vector2(shape.position.x, shape.end.y),
				shape.end,
				Vector2(shape.end.x, shape.position.y)
			])
			occuluder.occluder = polygon

func _on_child_order_changed():
	add_sprites()

func _ready():
	add_sprites()
	update_sprites()

func _process(delta):
	if Engine.is_editor_hint():
		update_sprites()
"

